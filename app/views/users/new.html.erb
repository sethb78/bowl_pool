<div class="signup">
	<% provide(:title, 'Sign up') %>
	<h1>Sign up</h1>
	<h5> *Indicates Mandatory Field<h3>
	<div class="row">
	  <div class="span6 offset3">
	    <%= form_for(@user) do |f| %>
	      <%= render 'fields', f: f %>
	      <%= f.submit "Create my account", class: "btn btn-large btn-primary" %>
	    <% end %>
	  </div>
	</div>
</div>
